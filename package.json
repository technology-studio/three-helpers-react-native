{
  "name": "@txo/three-helpers-react-native",
  "version": "1.1.1",
  "description": "Technology Studio - Three helpers react native",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/three-helpers-react-native.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn rollup -c --bundleConfigAsCjs",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky && yarn build && cp src/index.d.ts lib",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint:ci && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh",
    "lint:ci": "yarn lint",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.11",
    "@react-three/drei": "^9.114.4",
    "@react-three/fiber": "^8.17.10",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@txo-peer-dep/log": "^4.0.4",
    "@txo/commitlint": "^1.0.19",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^2.0.8",
    "@txo/tsconfig": "^1.1.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.11",
    "@types/three": "^0.169.0",
    "concurrently": "^9.0.1",
    "eslint-config-txo-typescript-react": "^2.0.78",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "npm-check-updates": "^17.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redux-saga": "^1.3.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "three": "^0.169.0",
    "three-stdlib": "^2.33.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@react-three/fiber": "*",
    "react": "*",
    "three": "*"
  }
}
