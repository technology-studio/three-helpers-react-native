{
  "name": "@txo/three-helpers-react-native",
  "version": "1.1.0",
  "description": "Technology Studio - Three helpers react native",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/three-helpers-react-native.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn rollup -c --bundleConfigAsCjs",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build && cp src/index.d.ts lib",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@microsoft/api-extractor": "^7.38.3",
    "@react-three/drei": "^9.88.16",
    "@react-three/fiber": "^8.15.11",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@txo-peer-dep/log": "^4.0.3",
    "@txo/commitlint": "^1.0.7",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^1.0.3",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/three": "^0.158.2",
    "concurrently": "^8.2.2",
    "eslint-config-txo-typescript-react": "^2.0.5",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "npm-check-updates": "^16.14.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redux-saga": "^1.2.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "three": "^0.158.0",
    "three-stdlib": "^2.28.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@react-three/fiber": "*",
    "react": "*",
    "three": "*"
  }
}
